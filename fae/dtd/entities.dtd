<!-- entitites.dtd -->

<!--
result-id abbreviations:
    cnt:    count
    tot:    total
    pct:    percent
    avg:    average
    b:      boolean
    r:      result (other)
-->

<!ENTITY % result-id
   "cnt1 |
    cnt2 |
    cnt3 |

    tot1 |
    tot2 |
    tot3 |

    pct1 |
    pct2 |
    pct3 |

    avg1 |
    avg2 |
    avg3 |

    b1 |
    b2 |
    b3 |

    r1 |
    r2 |
    r3 |
    r4 |
    r5 |
    r6 |
    r7 |
    r8 |
    r9 |
    r10 |
    r11 |
    r12"
>
